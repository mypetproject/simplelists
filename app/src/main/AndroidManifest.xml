<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appsverse.SimpleLists">

    <application
        android:allowBackup="true"
       android:icon="@mipmap/ic_round_goose5"

        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_round_goose5_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.appsverse.SimpleLists.App">

        <activity android:name="com.appsverse.SimpleLists.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="com.appsverse.SimpleLists.SettingsActivity"
            android:label="@string/action_settings"
            android:parentActivityName="com.appsverse.SimpleLists.MainActivity">
        </activity>
    </application>




</manifest>